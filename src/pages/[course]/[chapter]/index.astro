---
import { getStaticPathFromContentType } from "$utils"

interface Props {
	firstPage: string
}
const { firstPage } = Astro.props

export async function getStaticPaths() {
	return await getStaticPathFromContentType("chapter")
}
---

<meta http-equiv="refresh" content={`0;url=${Astro.url.href}/${firstPage}`} />
