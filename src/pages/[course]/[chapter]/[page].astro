---
import type { PageInstance } from "$data"
import ContentLayout from "$layouts/ContentLayout.astro"
import { getStaticPathFromContentType } from "$utils"

interface Props {
	page: PageInstance
}
const { page } = Astro.props

export async function getStaticPaths() {
	return getStaticPathFromContentType("page")
}
---

<ContentLayout content={page} />
